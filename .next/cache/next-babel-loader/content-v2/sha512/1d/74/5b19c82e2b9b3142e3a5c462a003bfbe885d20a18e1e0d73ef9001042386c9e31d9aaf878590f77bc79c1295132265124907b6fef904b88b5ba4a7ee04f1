ct, useState } from \"react\";\r\nimport AddTableau from \"../../components/AddTableau\";\r\n\r\ninterface ITableau{\r\n    id: number,\r\n    name: string\r\n}\r\n\r\nconst Dashboard = () => {\r\n    const [tableaux, setTableaux] = useState<ITableau[]>();\r\n\r\n    useEffect(() => {\r\n        fetchTableaux();\r\n    }, []);\r\n\r\n    const fetchTableaux = async () => {\r\n        const response = await axios.get('http://localhost:3300/api/tableaux');\r\n        const data: ITableau[] = response.data;\r\n        setTableaux(data);\r\n    };\r\n\r\n    const onAddTableau = async (tableauName: string) => {\r\n        const result = await apiAddTableau(tableauName);\r\n        if(result){\r\n            const newTab = [...tableaux];\r\n            newTab.push(tableauName);\r\n            setTableaux(newTab);\r\n        }\r\n    }\r\n\r\n    const apiAddTableau = async(tableauName: string) => {\r\n        const result = await axios.post('http://localhost:3300/api/tableaux/new', {\r\n            name: tableauName\r\n        });\r\n        try{\r\n            if(result.status === 400) throw new Error('Erreur lors de la création du tableau.');\r\n            return true;\r\n        } catch(e){\r\n            return false;\r\n        }\r\n    };\r\n\r\n    return (\r\n        <div>\r\n            { tableaux.length && tableaux.map((tableau, index) => (\r\n                <button key={index}><Link href={`/tableaux/${index}`}>{tableau}</Link></button>\r\n            ))}\r\n            <AddTableau onSubmit={onAddTableau}/>\r\n        </div>\r\n    )\r\n};\r\n\r\nexport default Dashboard;"]},"metadata":{},"sourceType":"module"}